#: nl/customer.csv:1
msgid "%s - Account creation"
msgstr "%s - Account gemaakt"

#: nl/customer.csv:2
msgid "%s - Your new password"
msgstr "%s - Je nieuwe wachtwoord"

#: nl/customer.csv:3
msgid "A request to reset your password has been made."
msgstr "Een verzoek om uw wachtwoord te resetten is gemaakt."

#: nl/customer.csv:4
msgid "An error occurred while connecting to your Facebook account. Please try again later"
msgstr "Er is een fout opgetreden tijdens het verbinden met je Facebook-account. Probeer het later opnieuw"

#: nl/customer.csv:5
msgid "Are you sure you want to remove this user?"
msgstr "Weet u zeker dat u deze gebruiker wilt verwijderen?"

#: nl/customer.csv:6
msgid "Change Password"
msgstr "Wachtwoord wijzigen"

#: nl/customer.csv:7
msgid "Congratulations,"
msgstr "Gefeliciteerd,"

#: nl/customer.csv:8
msgid "Create"
msgstr "Maak"

#: nl/customer.csv:9
