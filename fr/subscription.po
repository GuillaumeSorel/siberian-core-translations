#: fr/subscription.csv:1 
msgid "%s Subscription for %s"
msgstr "%s Abonnement pour %s"

#: fr/subscription.csv:2 
msgid "<a href="%s">Click here</a> to go back to your applications list"
msgstr "<a href="%s">Cliquez ici</a> pour retourner à la liste de vos applications"

#: fr/subscription.csv:3 
msgid "Active"
msgstr "Actif"

#: fr/subscription.csv:4 
msgid "An error occurred while processing your order. Please, try again later."
msgstr "Une erreur est survenue en traitant votre commande. Veuillez essayer ultérieurement."

#: fr/subscription.csv:5 
msgid "An error occurred while saving. Please try again later"
msgstr "Une erreur est survenue en sauvegardant. Veuillez essayer ultérieurement."

#: fr/subscription.csv:6 
msgid "An error occurred while saving. The selected subscription is not valid."
msgstr "Une erreur est survenue en sauvegardant. L'abonnement sélectionné n'est pas valide."

#: fr/subscription.csv:7 
msgid "Are you sure you want to delete this plan?"
msgstr "Etes-vous sur de vouloir supprimer ce plan ?"

#: fr/subscription.csv:8 
msgid "Billing Address"
msgstr "Adresse de facturation"

#: fr/subscription.csv:9 
msgid "By checking this box, you agree <a id="show_tos" href="javascript:void(0);" target="_blank" class="underline">to our terms and conditions</a>"
msgstr "En cochant cette case, vous acceptez <a id="show_tos" href="javascript:void(0);" target="_blank" class="underline">nos termes et conditions</a>"

#: fr/subscription.csv:10 
msgid "Charged to your client every month"
msgstr "Facturé à votre client tous les mois"

