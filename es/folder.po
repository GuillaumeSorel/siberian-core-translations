#: es/folder.csv:1
msgid "Folders"
msgstr "Carpetas"

#: es/folder.csv:2
msgid "Add a page"
msgstr "Agregar una página"

#: es/folder.csv:3
msgid "Click on a page to add it to the folder <span id=\"folder_name\" class=\"folder_name\">\"%s\"</span>"
msgstr "Haga clic en una página para agregarla a la carpeta <span id=\"folder_name\" class=\"folder_name\">\"%s\"</span>"

#: es/folder.csv:4
msgid "Click on"
msgstr "Clic en"

#: es/folder.csv:5
msgid "to add a folder."
msgstr "para agregar una carpeta."

#: es/folder.csv:6
msgid "to add a subfolder."
msgstr "añadir una subcarpeta."

#: es/folder.csv:7
msgid "New folder"
msgstr "Nueva carpeta"

#: es/folder.csv:8
msgid "Delete this folder and all its subfolders?"
msgstr "¿ Eliminar esta carpeta y todas las subcarpetas ?"

#: es/folder.csv:9
msgid "You will lose all unsaved changes, continue?"
msgstr "¿Perderá todos los cambios no guardados, desea continuar?"

#: es/folder.csv:10
msgid "You must first complete the current category creation process"
msgstr "Tiene que finalizar la creación de esta categoría primero"

#: es/folder.csv:11
msgid "Subtitle"
msgstr "Subtítulo"

#: es/folder.csv:12
msgid "Add a"
msgstr "Añadir una"

#: es/folder.csv:13
msgid "Mobile display"
msgstr "Visualización móvil"

#: es/folder.csv:14
msgid "Close mobile display"
msgstr "Cerrar visualización móvil"

#: es/folder.csv:15
msgid "Folder title is required"
msgstr "Se requiere el título de la carpeta"

#: es/folder.csv:16
msgid "You cannot add this feature"
msgstr "No se puede agregar esta función"
